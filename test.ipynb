    "import schoonmaken\n",
    "from datetime import date\n",
    "\n",
    "weeks = schoonmaken.weeks(date.today(), date(2022, 12, 30))\n",
    "schedule = schoonmaken.fill_schedule(weeks)\n",
    "# print(list(schedule))\n",
    "# print(len(list(schedule)))\n",
    "\n",
    "table = schoonmaken.build_table(schedule)\n",
    "table"
